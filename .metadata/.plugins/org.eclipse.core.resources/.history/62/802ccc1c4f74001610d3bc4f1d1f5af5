package co.edu.usa.df;

public class AtribDat {


	private String nombre;
	private String tipo;
	private int ancho;
	
	public AtribDat(String tipo, int ancho, int posicion) {
		this.tipo = tipo;
		this.ancho = ancho;
		this.posicion = posicion;
	}
	
	public String getTipo() {
		return tipo;
	}
	
	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
	
	public int getAncho() {
		return ancho;
	}
	
	public void setAncho(int ancho) {
		this.ancho = ancho;
	}
	
	public int getPosicion() {
		return posicion;
	}

	public void setPosicion(int posicion) {
		this.posicion = posicion;
	}

	@Override
	public String toString() {
		return "AtribDat [tipo=" + tipo + ", ancho=" + ancho + ", posicion=" + posicion + "]";
	}
}
