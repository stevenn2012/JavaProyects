package co.edu.usa.df;

public class AtribDat {

	private String tipo;
	private int ancho;
	private int posicion;
	
	public AtribDat(String tipo, int ancho) {
		this.tipo = tipo;
		this.ancho = ancho;
	}
	
	public String getTipo() {
		return tipo;
	}
	
	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
	
	public int getAncho() {
		return ancho;
	}
	
	public void setAncho(int ancho) {
		this.ancho = ancho;
	}
	
	@Override
	public String toString() {
		return "AtribDat [tipo=" + tipo + ", ancho=" + ancho + "]";
	}
}
